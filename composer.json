{
    "name": "conedevelopment/root",
    "description": "Another CMS.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Cone Development",
            "email": "hello@conedevelopment.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cone\\Root\\": "src/",
            "Cone\\Root\\Database\\Seeders\\": "database/seeders/",
            "Cone\\Root\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cone\\Root\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-gd": "*",
        "ext-exif": "*",
        "php": "^8.0 | ^8.1",
        "laravel/framework": "^8.75",
        "inertiajs/inertia-laravel": "^0.4.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "laravel/laravel": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cone\\Root\\RootServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
